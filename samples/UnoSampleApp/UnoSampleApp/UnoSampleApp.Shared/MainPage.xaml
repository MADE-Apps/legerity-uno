<Page
    x:Class="UnoSampleApp.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <ScrollViewer Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel HorizontalAlignment="Stretch">
            <AppBarButton
                x:Uid="SampleAppBarButton"
                Margin="0,0,0,12"
                Icon="Add"
                Label="Add" />
            <AppBarToggleButton
                x:Uid="SampleAppBarToggleButton"
                Margin="0,0,0,12"
                Icon="Shuffle"
                Label="SymbolIcon" />
            <AutoSuggestBox
                x:Uid="SampleAutoSuggestBox"
                Width="300"
                Margin="0,0,0,12"
                AutomationProperties.Name="Basic AutoSuggestBox"
                SuggestionChosen="AutoSuggestBox_SuggestionChosen"
                TextChanged="AutoSuggestBox_TextChanged" />
            <Button
                x:Uid="SampleButton"
                Margin="0,0,0,12"
                Content="Standard XAML button" />
            <CalendarDatePicker
                x:Uid="SampleCalendarDatePicker"
                Margin="0,0,0,12"
                Header="Calendar"
                PlaceholderText="Pick a date" />
            <CalendarView
                x:Uid="SampleCalendarView"
                Margin="0,0,0,12"
                CalendarIdentifier="GregorianCalendar"
                IsGroupLabelVisible="True"
                IsOutOfScopeEnabled="True"
                Language="en"
                SelectionMode="Single" />
            <CheckBox
                x:Uid="SampleCheckBox"
                Margin="0,0,0,12"
                Content="Two-state CheckBox" />
            <ComboBox
                x:Uid="SampleComboBox"
                Width="200"
                Margin="0,0,0,12"
                Header="Colors"
                PlaceholderText="Pick a color">
                <x:String>Blue</x:String>
                <x:String>Green</x:String>
                <x:String>Red</x:String>
                <x:String>Yellow</x:String>
            </ComboBox>
            <CommandBar
                x:Uid="SampleCommandBar"
                Margin="0,0,0,12"
                Background="Transparent"
                DefaultLabelPosition="Right"
                IsOpen="False">
                <AppBarButton Icon="Add" Label="Add" />
                <AppBarButton Icon="Edit" Label="Edit" />
                <AppBarButton Icon="Share" Label="Share" />
                <CommandBar.SecondaryCommands>
                    <AppBarButton Icon="Setting" Label="Settings">
                        <AppBarButton.KeyboardAccelerators>
                            <KeyboardAccelerator Key="I" Modifiers="Control" />
                        </AppBarButton.KeyboardAccelerators>
                    </AppBarButton>
                </CommandBar.SecondaryCommands>
            </CommandBar>
            <DatePicker
                x:Uid="SampleDatePicker"
                Margin="0,0,0,12"
                Header="Pick a date" />
            <GridView
                x:Name="BasicGridView"
                x:Uid="SampleGridView"
                Margin="0,0,0,12"
                IsItemClickEnabled="True"
                SelectionMode="Single" />
            <HyperlinkButton
                x:Uid="SampleHyperlinkButton"
                Margin="0,0,0,12"
                Content="Microsoft home page"
                NavigateUri="http://www.microsoft.com" />
            <ListBox
                x:Uid="SampleListBox"
                Width="200"
                Margin="0,0,0,12">
                <x:String>Blue</x:String>
                <x:String>Green</x:String>
                <x:String>Red</x:String>
                <x:String>Yellow</x:String>
            </ListBox>
            <ListView
                x:Name="BaseExample"
                x:Uid="SampleListView"
                Width="350"
                Height="400"
                Margin="0,0,0,12"
                HorizontalAlignment="Left"
                BorderBrush="{ThemeResource SystemControlForegroundBaseMediumLowBrush}"
                BorderThickness="1" />
            <PasswordBox
                x:Uid="SamplePasswordBox"
                Width="300"
                Margin="0,0,0,12"
                AutomationProperties.Name="Simple PasswordBox" />
            <Pivot
                x:Uid="SamplePivot"
                Title="EMAIL"
                Margin="0,0,0,12">
                <PivotItem Header="All">
                    <TextBlock Text="all emails go here." />
                </PivotItem>
                <PivotItem Header="Unread">
                    <TextBlock Text="unread emails go here." />
                </PivotItem>
                <PivotItem Header="Flagged">
                    <TextBlock Text="flagged emails go here." />
                </PivotItem>
                <PivotItem Header="Urgent">
                    <TextBlock Text="urgent emails go here." />
                </PivotItem>
            </Pivot>
            <ProgressBar
                x:Uid="SampleProgressBar"
                Width="130"
                Margin="0,0,0,12"
                IsIndeterminate="True"
                ShowError="False"
                ShowPaused="False" />
            <ProgressRing
                x:Uid="SampleProgressRing"
                Margin="0,0,0,12"
                IsActive="True" />
            <RadioButton
                x:Uid="SampleRadioButton1"
                Margin="0,0,0,12"
                Content="Option 1"
                GroupName="Options" />
            <RadioButton
                x:Uid="SampleRadioButton2"
                Margin="0,0,0,12"
                Content="Option 2"
                GroupName="Options" />
            <RadioButton
                x:Uid="SampleRadioButton3"
                Margin="0,0,0,12"
                Content="Option 3"
                GroupName="Options" />
            <Slider
                x:Uid="SampleSlider"
                Width="200"
                Margin="0,0,0,12"
                LargeChange="100"
                Maximum="1000"
                Minimum="500"
                SmallChange="10"
                StepFrequency="10"
                Value="800" />
            <TextBlock
                x:Uid="SampleTextBlock"
                Margin="0,0,0,12"
                Text="I am a TextBlock" />
            <TextBox
                x:Uid="SampleTextBox"
                Margin="0,0,0,12"
                AutomationProperties.Name="simple TextBox" />
            <TimePicker x:Uid="SampleTimePicker" Margin="0,0,0,12" />
            <ToggleButton
                x:Uid="SampleToggleButton"
                Margin="0,0,0,12"
                Content="ToggleButton" />
            <ToggleSwitch
                x:Uid="SampleToggleSwitch"
                Margin="0,0,0,12"
                AutomationProperties.Name="simple ToggleSwitch" />
        </StackPanel>
    </ScrollViewer>
</Page>
